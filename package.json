{
  "name": "chrome-qr-scanner",
  "version": "1.0.0",
  "description": "A Chrome extension that allows you to scan QR codes and open links in new tabs.",
  "private": true,
  "scripts": {
    "build": "npm run package",
    "package": "npm run webpack:package",
    "watch": "npm run webpack:dev:watch",
    "webpack": "npm run webpack:prod",
    "webpack:dev": "webpack --config webpack.dev.js",
    "webpack:dev:watch": "webpack --watch --config webpack.dev.js",
    "webpack:package": "webpack --config webpack.package.js",
    "webpack:prod": "webpack --config webpack.prod.js",
    "prepare": "husky install",
    "prettier": "prettier --write \"src/{styles,ts,html}/**/*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheAppleFreak/chrome-qr-scanner.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "qr",
    "scanner",
    "generator"
  ],
  "author": "TheAppleFreak <TheAppleFreak@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheAppleFreak/chrome-qr-scanner/issues"
  },
  "homepage": "https://github.com/TheAppleFreak/chrome-qr-scanner#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-react-constant-elements": "^7.14.5",
    "@babel/plugin-transform-react-inline-elements": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@types/chrome": "^0.0.147",
    "@types/holderjs": "^2.9.1",
    "@types/node": "^16.3.3",
    "@types/qrcode": "^1.4.1",
    "@types/react-dom": "^17.0.9",
    "@types/zip-webpack-plugin": "^3.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.1.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "make-dir-cli": "^3.0.0",
    "mini-css-extract-plugin": "^2.1.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "node-sass": "^6.0.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.1.0",
    "terser": "^5.7.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-extension-manifest-plugin": "^0.7.0",
    "webpack-merge": "^5.8.0",
    "zip-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^4.1.17",
    "holderjs": "^2.9.9",
    "loglevelnext": "^5.0.5",
    "qr-scanner": "^1.2.0",
    "qrcode": "^1.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
