{
  "name": "chrome-qr-scanner",
  "version": "0.1.0",
  "description": "A Chrome extension that allows you to create and scan QR codes using your webcam.",
  "private": true,
  "scripts": {
    "build": "npm run webpack",
    "extract": "npx i18next-parser -c i18next-parser.config.js",
    "watch": "npm run webpack:dev:watch",
    "webpack": "npm run webpack:prod",
    "webpack:analysis": "webpack --watch --config webpack.analysis.js",
    "webpack:dev": "webpack --config webpack.dev.js",
    "webpack:dev:watch": "webpack --watch --config webpack.dev.js",
    "webpack:prod": "webpack --config webpack.prod.js",
    "prepare": "husky install",
    "prettier": "prettier --write \"src/{styles,ts,html}/**/*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheAppleFreak/chrome-qr-scanner.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "qr",
    "scanner",
    "generator"
  ],
  "author": "TheAppleFreak <TheAppleFreak@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheAppleFreak/chrome-qr-scanner/issues"
  },
  "homepage": "https://github.com/TheAppleFreak/chrome-qr-scanner#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.14.5",
    "@babel/plugin-transform-react-inline-elements": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@types/chrome": "^0.0.147",
    "@types/lodash": "^4.14.171",
    "@types/md5": "^2.3.1",
    "@types/node": "^16.3.3",
    "@types/qrcode": "^1.4.1",
    "@types/react-dom": "^17.0.9",
    "@types/react-select": "^4.0.17",
    "@types/zip-webpack-plugin": "^3.0.3",
    "ajv-cli": "^5.0.0",
    "ajv-json-loader": "^0.1.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.1.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "i18next-parser": "^4.2.0",
    "make-dir-cli": "^3.0.0",
    "mini-css-extract-plugin": "^2.1.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "node-sass": "^6.0.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.1.0",
    "terser": "^5.7.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-extension-manifest-plugin": "^0.7.0",
    "webpack-merge": "^5.8.0",
    "zip-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "ajv": "^6.12.6",
    "clipboard-polyfill": "^3.0.3",
    "i18next": "^20.3.3",
    "i18next-browser-languagedetector": "^6.1.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "qr-scanner": "^1.2.0",
    "qrcode": "^1.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.3",
    "react-icons": "^4.2.0",
    "react-select": "^4.3.1"
  }
}
